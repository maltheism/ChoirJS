{
  "name": "choirjs",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "serve": "node dist/www.js",
    "build": "babel src --out-dir dist",
    "production": "webpack --mode production",
    "development": "webpack --mode development",
    "babel-node": "babel-node --presets=@babel/preset-env",
    "start": "nodemon --delay 1500ms --exec babel-node ./bin/www.js",
    "server": "nodemon --delay 2000ms --exec npm run babel-node ./bin/www.js",
    "messages": "NODE_ENV=production babel src/jsx/* >/dev/null",
    "locales": "node ./translationRunner.js",
    "translate": "npm run messages; npm run locales"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~4.1.1",
    "dexie": "^2.0.4",
    "express": "~4.17.1",
    "express-session": "^1.16.2",
    "helmet": "^3.19.0",
    "helmet-csp": "^2.7.1",
    "http-errors": "^1.7.3",
    "ip": "^1.1.5",
    "jquery": "^3.4.1",
    "morgan": "~1.9.1",
    "pg-promise": "^8.7.4",
    "popper.js": "^1.15.0",
    "react": "^16.8.6",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-intl.macro": "^0.3.6",
